{
  "id": "php-profiling-onboarding-3-configure-profiling",
  "name": "PHP",
  "html": "<h4 id=\"configure-profiling\" style=\"position:relative;\">Configure Profiling</h4>\n<p>Add the <code>profiles_sample_rate</code> option to your SDK config.</p>\n<div class=\"gatsby-highlight\" data-language=\"php\"><pre class=\"language-php highlight\"><code class=\"language-php\"><span class=\"token function\"><span class=\"token punctuation\">\\</span>Sentry<span class=\"token punctuation\">\\</span>init</span><span class=\"token punctuation\">(</span><span class=\"token punctuation\">[</span>\n    <span class=\"token string single-quoted-string\">'profiles_sample_rate'</span> <span class=\"token operator\">=&gt;</span> <span class=\"token number\">1.0</span><span class=\"token punctuation\">,</span>\n<span class=\"token punctuation\">]</span><span class=\"token punctuation\">)</span><span class=\"token punctuation\">;</span></code></pre></div>",
  "link": "https://docs.sentry.io/platforms/php/profiling/",
  "wizard_setup": null
}
