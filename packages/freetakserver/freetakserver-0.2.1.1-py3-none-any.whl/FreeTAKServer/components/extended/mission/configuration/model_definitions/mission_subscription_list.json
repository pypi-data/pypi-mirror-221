{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://iec.ch/TC57/all_mission_subscriptions#",
	"type": "object",
	"properties":
	{
		"mission_subscription_list":
		{
			"type": "object",
			"anyOf":
			[
				{ "$ref": "#/definitions/MissionInfo" },
				{ "$ref": "#/definitions/MissionSubscription" },
                { "$ref": "#/definitions/MissionRole" }
			]
		}
	},
	"definitions":
	{
        "MissionInfo" : 
        {
            "type": "object",
            "description" : "",
            "properties":
            {
                "data":
				{
					"type" : "array",
					"items" : { "$ref":"#/definitions/MissionSubscription" }
				}
            }
        },
        "MissionSubscription": {
            "type" : "object",
            "description" : "",
            "properties": {
                "role":{ "$ref":"#/definitions/MissionRole" }
            }
        },
        "MissionRole":{
            "type": "object",
            "description" : "",
            "properties": {
            }
        }
	}
}
